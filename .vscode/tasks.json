{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/bin/cc",
            "args": [
                "-fdiagnostics-color=always",
                "-Wall",
                "-Wextra",
                "-Werror",
                "-g3",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}",
                "-I",
                "${workspaceFolder}/Minishell/include",
                "-L${workspaceFolder}/Minishell/libft",
                "-lft",
                "-lreadline",
                "-ltermcap"
            ],
            "options": {
                "cwd": "${workspaceFolder}/Minishell"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}